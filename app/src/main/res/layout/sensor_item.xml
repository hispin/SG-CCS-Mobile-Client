<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="70dp" xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tool="http://schemas.android.com/tools">


    <TextView
            android:id="@+id/tvId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tool:text="id"
            android:textSize="13.3sp"
            android:textColor="@color/dark1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.100"
            app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
            android:id="@+id/tvZone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tool:text="name"
            android:textSize="13.3sp"
            android:textColor="@color/dark1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.230"
            app:layout_constraintEnd_toEndOf="parent"/>

    <EditText
            android:id="@+id/etName"
            tool:text="dddd"
            android:hint="@string/rename"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="13.3sp"
            android:textColor="@color/dark1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.250"
            app:layout_constraintEnd_toEndOf="parent"
            android:visibility="invisible"
            android:inputType="text"
            android:imeOptions="actionDone"

            />

    <TextView
            android:id="@+id/tvType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/seismic"
            android:textColor="@color/dark1"
            android:textSize="13.3sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.450"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/ibEditName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_edit"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.940"
            android:background="@color/transparent"
            android:padding="2dp" />


    <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/ivIsLocate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.630"
            app:layout_constraintStart_toStartOf="parent"
            android:gravity="center"
            app:srcCompat="@drawable/ic_not_located" />
    <!--    <TextView-->
    <!--            android:id="@+id/tvIsLocate"-->
    <!--            android:layout_width="wrap_content"-->
    <!--            android:layout_height="wrap_content"-->
    <!--            android:text=""-->
    <!--            android:textSize="13.3sp"-->
    <!--            android:textColor="@color/red1"-->
    <!--            app:layout_constraintStart_toEndOf="@+id/ibEditName"-->
    <!--            app:layout_constraintTop_toTopOf="parent"-->
    <!--            app:layout_constraintBottom_toBottomOf="parent"-->
    <!--            android:layout_marginStart="25dp"-->
    <!--            android:gravity="center"-->
    <!--            android:drawablePadding="7.3dp"-->
    <!--            app:drawableStartCompat="@drawable/ic_not_located" />-->

    <ToggleButton
            android:id="@+id/togIsActive"
            android:layout_width="55dp"
            android:layout_height="wrap_content"
            android:button="@drawable/toggle_selector_active"
            android:gravity="center"
            android:background="@color/transparent"
            android:textOff=""
            android:textOn=""
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.870"
    />

